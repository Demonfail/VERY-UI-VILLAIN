var NAME,INST;

for (var i=0;i&lt;SIZE;i++)
{
    INST = guiELEMENT[# 0, SIZE-1 - i];
    NAME = INST.TITLE;
    
    draw_text_transformed(10, 200 + 42 * i + 5, string(i)+" : "+NAME+" , "+string(INST.depth), 0.7, 0.7, 0);
}
