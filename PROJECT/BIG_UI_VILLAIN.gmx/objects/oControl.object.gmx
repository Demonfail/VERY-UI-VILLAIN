//Debug thing
if (DEBUG_FPSCTR &lt; 360 &amp;&amp; ++DEBUG_FPSCTR &gt;= 360)
{
    DEBUG_FPSCTR = 0;
    DEBUG_MINFPS = fps_real;
    DEBUG_MAXFPS = fps_real;
}

if (fps_real &gt;= 0)
{
    if (fps_real &lt; DEBUG_MINFPS) DEBUG_MINFPS = fps_real;
    if (fps_real &gt; DEBUG_MAXFPS) DEBUG_MAXFPS = fps_real;
}

DEBUG_AVGFPS += fps_real;
DEBUG_AVGFPS /= 2;

var MINCOL = make_colour_hsv(lerp(0,42,(dsin(DEBUG_FPSCTR)+1)/2),255,255);
var MAXCOL = c_blue;
if ((DEBUG_FPSCTR mod 42) &lt;= 24) MAXCOL = c_yellow;

vui_draw();
draw_text(10,10,"FPS_RL "+string(fps_real));
draw_text_transformed_colour(20,42,"FPS_MIN "+string(DEBUG_MINFPS),0.5,0.5,0,MINCOL,MINCOL,MINCOL,MINCOL,1);
draw_text_transformed_colour(20,84,"FPS_MAX "+string(DEBUG_MAXFPS),0.5,0.5,0,MAXCOL,MAXCOL,MAXCOL,MAXCOL,1);
